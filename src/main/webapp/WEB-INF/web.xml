<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>ExamenProject</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <jsp-config>
    <taglib>
      <taglib-uri>http://java.sun.com/jsp/jstl/core</taglib-uri>
      <taglib-location>/WEB-INF/lib/jstl-impl.jar</taglib-location>
    </taglib>
  </jsp-config>
  <servlet>
    <description></description>
    <display-name>AthentificationServlet</display-name>
    <servlet-name>AthentificationServlet</servlet-name>
    <servlet-class>AthentificationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AthentificationServlet</servlet-name>
    <url-pattern>/AthentificationServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AdminServlet</display-name>
    <servlet-name>AdminServlet</servlet-name>
    <servlet-class>AdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminServlet</servlet-name>
    <url-pattern>/AdminServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>EnseignantServlet</display-name>
    <servlet-name>EnseignantServlet</servlet-name>
    <servlet-class>tn.iit.controller.EnseignantServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EnseignantServlet</servlet-name>
    <url-pattern>/EnseignantServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>TirageServlet</display-name>
    <servlet-name>TirageServlet</servlet-name>
    <servlet-class>tn.iit.controller.TirageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TirageServlet</servlet-name>
    <url-pattern>/TirageServlet</url-pattern>
  </servlet-mapping>
</web-app>